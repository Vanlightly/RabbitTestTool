{
    "topologyName": "LT_SafetyBenchmark3",
    "topologyType": "MultipleDimensions",
    "benchmarkType": "latency",
    "description": "1Kb msgs/s. Confirms (1000) and manual acks (500/1). Scale out publishers and consumers",
    "vhosts": [
        {
            "name": "benchmark",
            "scale": 1,
            "queueGroups": [
                {
                    "group": "q1",
                    "scale": 1
                }
            ],
            "publisherGroups": [
                {
                    "group": "p1",
                    "scale": 1,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 1000
                    },
                    "sendToQueueGroup": {
                        "queueGroup": "q1",
                        "mode": "random"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 1024,
                    "msgsPerSecondPerPublisher": 100
                }
            ],
            "consumerGroups": [
                {
                    "group": "c1",
                    "scale": 1,
                    "queueGroup": "q1",
                    "ackMode": {
                        "manualAcks": true,
                        "consumerPrefetch": 500,
                        "ackInterval": 1
                    }
                }
            ]
        }
    ],
    "dimensions" : {
        "multipleDimensions": {
            "dimensions": ["Consumers", "Publishers"],
            "multiValues": [
                [1,1],
                [2,2],
                [4,4],
                [8,8],
                [16,16],
                [32,32]
            ],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10
        }
    }
}