{
    "topologyName": "LT_SafetyBenchmark1",
    "topologyType": "SingleDimension",
    "benchmarkType": "latency",
    "description": "1Kb msgs/s. Confirms (1000) and manual acks (500/1). Scale out consumers",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 1
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 1000
                    },
                    "sendToQueuePrefix": {
                        "queuePrefix": "q1",
                        "mode": "random"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 1024,
                    "msgsPerSecondPerPublisher": 100
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 1,
                    "queuePrefix": "q1",
                    "ackMode": {
                        "manualAcks": true,
                        "consumerPrefetch": 500,
                        "ackInterval": 1
                    }
                }
            ]
        }
    ],
    "dimensions" : {
        "singleDimension": {
            "dimension": "Consumers",
            "values": [1, 2, 4, 8, 16, 32],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10
        }
    }
}