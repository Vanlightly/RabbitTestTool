{
    "topologyName": "LT_DirectBenchmark1",
    "topologyType": "SingleDimension",
    "benchmarkType": "latency",
    "description": "Single, short, fixed routing key",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "exchanges": [
                {
                    "name": "ex1",
                    "type": "direct"
                }
            ],
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 1,
                    "bindings": [
                        {
                            "from": "ex1",
                            "bindingKeys": ["rk1"]
                        }
                    ]
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "sendToExchange": {
                        "exchange": "ex1",
                        "routingKey": "rk1",
                        "routingKeyMode": "fixedvalue"
                    },
                    "deliveryMode": "Persistent",
                    "msgsPerSecondPerPublisher": 1
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 1,
                    "queuePrefix": "q1"
                }
            ]
        }
    ],
    "dimensions" : {
        "singleDimension": {
            "dimension": "PublisherRate",
            "values": [1, 10, 50, 100, 500, 1000, 5000, 10000],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10,
            "applyToGroup": "p1"
        }
    }
}