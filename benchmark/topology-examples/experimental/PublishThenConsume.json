{
    "topologyName": "PublishThenConsume",
    "topologyType": "MultipleDimensions",
    "benchmarkType": "throughput",
    "description": "Persistent, lazy, 32 kb, 1 pub/queue",
    "vhosts": [
        {
            "name": "benchmark",
            "scale": 1,
            "queueGroups": [
                {
                    "group": "q1",
                    "scale": 1
                }
            ],
            "publisherGroups": [
                {
                    "group": "p1",
                    "scale": 1,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 1000
                    },
                    "sendToQueueGroup": {
                        "queueGroup": "q1",
                        "mode": "Counterpart"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 32000,
                    "initialPublish": 10000
                }
            ],
            "consumerGroups": [
                {
                    "group": "c1",
                    "scale": 1,
                    "queueGroup": "q1",
                    "ackMode": {
                        "manualAcks": true,
                        "consumerPrefetch": 100,
                        "ackInterval": 1
                    }
                }
            ]
        }
    ],
    "dimensions" : {
        "multipleDimensions": {
            "dimensions": ["PublisherRate","ProcessingMs"],
            "multiValues": [
                [100000,1000],
                [1,0]
            ],
            "stepDurationSeconds": 300,
            "rampUpSeconds": 5
        }
    }
}