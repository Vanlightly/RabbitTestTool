{
    "topologyName": "PublishOnly6",
    "topologyType": "Fixed",
    "benchmarkType": "throughput",
    "description": "100 queues, one publisher per queue, 64Kb messages",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 100,
            "exchanges": [
                {
                    "name": "ex1",
                    "type": "fanout"
                }
            ],
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 1,
                    "bindings": [
                        { "from": "ex1" }
                    ]
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "sendToExchange": {
                        "exchange": "ex1",
                        "routingKeyMode": "none"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 65536
                }
            ]
        }
    ],
    "dimensions" : {
        "fixedDimensions": {
            "durationSeconds": 60,
            "rampUpSeconds": 10
        }
    }
}