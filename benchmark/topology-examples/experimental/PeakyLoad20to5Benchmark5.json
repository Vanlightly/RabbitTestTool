{
    "topologyName": "TP_PeakyLoad20to5Benchmark5",
    "topologyType": "SingleDimension",
    "benchmarkType": "throughput",
    "description": "1kb msgs. 20 publishers. Confirms (1000) and manual acks (500/1). Peaky pub rate",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 1
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 30,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 1000
                    },
                    "sendToQueuePrefix": {
                        "queuePrefix": "q1",
                        "mode": "random"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 1024,
                    "msgsPerSecondPerPublisher": 10
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 2,
                    "queuePrefix": "q1",
                    "ackMode": {
                        "manualAcks": true,
                        "consumerPrefetch": 500,
                        "ackInterval": 1
                    }
                }
            ]
        }
    ],
    "dimensions" : {
        "singleDimension": {
            "dimension": "PublisherRate",
            "values": [400, 1000, 1000, 400, 400, 400, 400],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10
        }
    }
}