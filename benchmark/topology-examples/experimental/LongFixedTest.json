{
  "topologyName": "TP_LongFixedTest",
  "topologyType": "fixed",
  "benchmarkType": "throughput",
  "description": "Persistent messages",
  "topologyGroups": [
    {
      "name": "benchmark",
      "scale": 1,
      "queues": [
          {
              "prefix": "q1",
              "scale": 1
          }
      ],
      "publishers": [
          {
              "prefix": "p1",
              "scale": 2,
              "publishMode": {
                  "useConfirms": true,
                  "inFlightLimit": 1000
              },
              "sendToQueuePrefix": {
                  "queuePrefix": "q1",
                  "mode": "random"
              },
              "deliveryMode": "Persistent",
              "messageSize": 1024
          }
      ],
      "consumers": [
          {
              "prefix": "c1",
              "scale": 1,
              "queuePrefix": "q1",
              "ackMode": {
                  "manualAcks": true,
                  "consumerPrefetch": 500,
                  "ackInterval": 1
              }
          }
      ]
    }
  ],
  "dimensions" : {
    "fixedDimensions": {
      "durationSeconds": 1200,
      "rampUpSeconds": 10
    }
  }
}