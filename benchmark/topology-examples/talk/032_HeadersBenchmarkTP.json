{
    "topologyName": "TP_HeadersBenchmark1.json",
    "topologyType": "Fixed",
    "benchmarkType": "throughput",
    "description": "1 header in binding, increasing header count",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "exchanges": [
                {
                    "name": "ex1",
                    "type": "headers"
                }
            ],
            "queues": [
                {
                    "prefix": "q1", "scale": 1,
                    "bindings": [
                        {
                            "from": "ex1",
                            "properties": [
                                { "key": "x-match", "value": "any" },
                                { "key": "c1", "value": "a", "type": "string"  }
                            ]
                        }
                    ]
                },
                {
                    "prefix": "q2", "scale": 1,
                    "bindings": [
                        {
                            "from": "ex1",
                            "properties": [
                                { "key": "x-match", "value": "any" },
                                { "key": "c2", "value": 1000000, "type": "int" }
                            ]
                        }
                    ]
                },
                {
                    "prefix": "q3", "scale": 1,
                    "bindings": [
                        {
                            "from": "ex1",
                            "properties": [
                                { "key": "x-match", "value": "any" },
                                { "key": "c3", "value": "b", "type": "string" }
                            ]
                        }
                    ]
                },
                {
                    "prefix": "q4", "scale": 1,
                    "bindings": [
                        {
                            "from": "ex1",
                            "properties": [
                                { "key": "x-match", "value": "any" },
                                { "key": "c4", "value": 2000000, "type": "int" }
                            ]
                        }
                    ]
                }
                ,
                {
                    "prefix": "q5", "scale": 1,
                    "bindings": [
                        {
                            "from": "ex1",
                            "properties": [
                                { "key": "x-match", "value": "any" },
                                { "key": "c5", "value": "c", "type": "string" }
                            ]
                        }
                    ]
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "sendToExchange": {
                        "exchange": "ex1",
                        "routingKeyMode": "none"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 16,
                    "availableHeaders": [
                        { "key": "c1", "value": "a", "type": "string" },
                        { "key": "c2", "value": 1000000, "type": "int" },
                        { "key": "c3", "value": "b", "type": "string" },
                        { "key": "c4", "value": 2000000, "type": "int" },
                        { "key": "c5", "value": "c", "type": "string" }
                    ],
                    "headersPerMessage": 1
                }
            ],
            "consumers": [
                { "prefix": "c1", "scale": 1, "queuePrefix": "q1" },
                { "prefix": "c2", "scale": 1, "queuePrefix": "q2" },
                { "prefix": "c3", "scale": 1, "queuePrefix": "q3" },
                { "prefix": "c4", "scale": 1, "queuePrefix": "q4" },
                { "prefix": "c5", "scale": 1, "queuePrefix": "q5" }
            ]
        }
    ],
    "dimensions" : {
        "fixedDimensions": {
            "durationSeconds": 300,
            "rampUpSeconds": 10
          }
    }
}