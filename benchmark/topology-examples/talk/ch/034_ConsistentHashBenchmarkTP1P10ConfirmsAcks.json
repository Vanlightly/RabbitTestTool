{
  "topologyName": "FastConsistentHash",
  "topologyType": "fixed",
  "benchmarkType": "throughput",
  "vhosts": [
    {
      "name": "benchmark",
      "scale": 1,
      "exchanges": [
        {
          "name": "ex1",
          "type": "consistenthash"
        }
      ],
      "queueGroups": [
        {
          "group": "q1",
          "scale": 1,
          "bindings": [
            {
              "from": "ex1",
              "bindingKey": "1"
            }
          ]
        }
      ],
      "publisherGroups": [
        {
          "group": "p1",
          "scale": 10,
          "publishMode": {
            "useConfirms": true,
            "inFlightLimit": 10000
          },
          "sendToExchange": {
            "exchange": "ex1",
            "routingKeyMode": "random"
          },
          "deliveryMode": "Persistent"
        }
      ],
      "consumerGroups": [
        {
          "group": "c1",
          "scale": 1,
          "queueGroup": "q1",
          "ackMode": {
              "manualAcks": true,
              "consumerPrefetch": 1000,
              "ackInterval": 100
          }
        }
      ]
    }
  ],
  "dimensions" : {
    "fixedDimensions": {
      "durationSeconds": 120,
      "rampUpSeconds": 10
    }
  }
}