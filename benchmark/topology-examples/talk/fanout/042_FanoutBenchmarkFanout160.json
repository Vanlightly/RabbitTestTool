{
  "topologyName": "TP_DirectBenchmark2",
  "topologyType": "Fixed",
  "benchmarkType": "throughput",
  "description": "5 routing keys, five queues",
  "topologyGroups": [
    {
      "name": "benchmark",
      "scale": 1,
      "exchanges": [
        {
          "name": "ex1",
          "type": "fanout"
        }
      ],
      "queues": [
        { "prefix": "q1", "scale": 160, "bindings": [ { "from": "ex1" } ]}
      ],
      "publishers": [
        {
          "prefix": "p1",
          "scale": 1,
          "sendToExchange": {
            "exchange": "ex1",
            "routingKeyMode": "none"
          },
          "deliveryMode": "Persistent",
          "msgsPerSecondPerPublisher": 1000
        }
      ],
      "consumers": [
        { "prefix": "c1", "scale": 160, "queuePrefix": "q1" }
      ]
    }
  ],
  "dimensions" : {
    "fixedDimensions": {
      "durationSeconds": 120,
      "rampUpSeconds": 10
    }
  }
}