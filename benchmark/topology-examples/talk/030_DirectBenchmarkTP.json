{
  "topologyName": "TP_DirectBenchmark2",
  "topologyType": "fixed",
  "benchmarkType": "throughput",
  "description": "5 routing keys, five queues",
  "topologyGroups": [
    {
      "name": "benchmark",
      "scale": 1,
      "exchanges": [
        {
          "name": "ex1",
          "type": "direct"
        }
      ],
      "queues": [
        { "prefix": "q1", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["rkey1"] } ] },
        { "prefix": "q2", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["rkey2"] } ] },
        { "prefix": "q3", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["rkey3"] } ] },
        { "prefix": "q4", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["rkey4"] } ] },
        { "prefix": "q5", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["rkey5"] } ] }
      ],
      "publishers": [
        {
          "prefix": "p1",
          "scale": 1,
          "sendToExchange": {
            "exchange": "ex1",
            "routingKeys": ["rkey1", "rkey2", "rkey3", "rkey4", "rkey5"],
            "routingKeyMode": "multivalue"
          },
          "deliveryMode": "Persistent"
        }
      ],
      "consumers": [
        { "prefix": "c1", "scale": 1, "queuePrefix": "q1" },
        { "prefix": "c2", "scale": 1, "queuePrefix": "q2" },
        { "prefix": "c3", "scale": 1, "queuePrefix": "q3" },
        { "prefix": "c4", "scale": 1, "queuePrefix": "q4" },
        { "prefix": "c5", "scale": 1, "queuePrefix": "q5" }
      ]
    }
  ],
  "dimensions" : {
    "fixedDimensions": {
      "durationSeconds": 300,
      "rampUpSeconds": 10
    }
  }
}