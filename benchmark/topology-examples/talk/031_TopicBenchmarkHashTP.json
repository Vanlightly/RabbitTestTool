{
  "topologyName": "TP_DirectBenchmark2",
  "topologyType": "fixed",
  "benchmarkType": "throughput",
  "description": "5 routing keys, five queues",
  "topologyGroups": [
    {
      "name": "benchmark",
      "scale": 1,
      "exchanges": [
        {
          "name": "ex1",
          "type": "topic"
        }
      ],
      "queues": [
        { "prefix": "q1", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["#.1"] } ] },
        { "prefix": "q2", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["#.2"] } ] },
        { "prefix": "q3", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["#.3"] } ] },
        { "prefix": "q4", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["#.4"] } ] },
        { "prefix": "q5", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["#.5"] } ] }
      ],
      "publishers": [
        {
          "prefix": "p1",
          "scale": 1,
          "sendToExchange": {
            "exchange": "ex1",
            "routingKeys": ["r.k.1", "r.k.2", "r.k.3", "r.k.4", "r.k.5"],
            "routingKeyMode": "multivalue"
          },
          "deliveryMode": "Persistent"
        }
      ],
      "consumers": [
        { "prefix": "c1", "scale": 1, "queuePrefix": "q1" },
        { "prefix": "c2", "scale": 1, "queuePrefix": "q2" },
        { "prefix": "c3", "scale": 1, "queuePrefix": "q3" },
        { "prefix": "c4", "scale": 1, "queuePrefix": "q4" },
        { "prefix": "c5", "scale": 1, "queuePrefix": "q5" }
      ]
    }
  ],
  "dimensions" : {
    "fixedDimensions": {
      "durationSeconds": 300,
      "rampUpSeconds": 10
    }
  }
}