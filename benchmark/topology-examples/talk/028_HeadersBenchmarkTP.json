{
    "topologyName": "TP_HeadersBenchmark1.json",
    "topologyType": "Fixed",
    "benchmarkType": "throughput",
    "description": "1 header in binding, increasing header count",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "exchanges": [
                {
                    "name": "ex1",
                    "type": "headers"
                }
            ],
            "queues": [
                {
                    "prefix": "q1", "scale": 1,
                    "bindings": [
                        {
                            "from": "ex1",
                            "properties": [
                                { "key": "x-match", "value": "any" },
                                { "key": "c1", "value": "a", "type": "string"  }
                            ]
                        }
                    ]
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "sendToExchange": {
                        "exchange": "ex1",
                        "routingKeyMode": "none"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 16,
                    "availableHeaders": [
                        { "key": "c1", "value": "a", "type": "string" }
                    ],
                    "headersPerMessage": 1
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 1,
                    "queuePrefix": "q1"
                }
            ]
        }
    ],
    "dimensions" : {
        "fixedDimensions": {
            "durationSeconds": 300,
            "rampUpSeconds": 10
          }
    }
}