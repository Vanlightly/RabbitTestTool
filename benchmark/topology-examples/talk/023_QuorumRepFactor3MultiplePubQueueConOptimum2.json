{
    "topologyName": "TALK_020_MirroredRepFactor3MultiplePubQueueCon",
    "topologyType": "MultipleDimensions",
    "benchmarkType": "throughput",
    "description": "Quorum (3x), 1 kb, 500 msg/s, increasing pub/queue/consumer",
    "vhosts": [
        {
            "name": "benchmark",
            "scale": 1,
            "queueGroups": [
                {
                    "group": "q1",
                    "scale": 1,
                    "properties": [
                        { "key": "x-queue-type", "value": "quorum", "type": "string" },
                        { "key": "x-quorum-initial-group-size", "value": "3", "type": "int" }
                      ]
                }
            ],
            "publisherGroups": [
                {
                    "group": "p1",
                    "scale": 1,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 10000
                    },
                    "sendToQueueGroup": {
                        "queueGroup": "q1",
                        "mode": "Counterpart"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 1024
                }
            ],
            "consumerGroups": [
                {
                    "group": "c1",
                    "scale": 1,
                    "queueGroup": "q1",
                    "ackMode": {
                        "manualAcks": true,
                        "consumerPrefetch": 10000,
                        "ackInterval": 1
                    }
                }
            ]
        }
    ],
    "dimensions" : {
        "multipleDimensions": {
            "dimensions": ["Queues","Consumers", "Publishers"],
            "multiValues": [
                [1,1,1],
                [2,2,2],
                [3,3,3],
                [4,4,4],
                [5,5,5],
                [6,6,6],
                [7,7,7],
                [8,8,8],
                [9,9,9],
                [10,10,10]
            ],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 5
        }
    }
}