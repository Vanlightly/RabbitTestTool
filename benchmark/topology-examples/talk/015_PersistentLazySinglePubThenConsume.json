{
    "topologyName": "TALK_016_PersistentLazySinglePubOnly",
    "topologyType": "MultipleDimensions",
    "benchmarkType": "throughput",
    "description": "Persistent, lazy, 32 kb, 1 pub/queue",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 1,
                    "properties": [
                        { "key": "x-queue-mode", "value": "lazy", "type": "string" }
                      ]
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "sendToQueuePrefix": {
                        "queuePrefix": "q1",
                        "mode": "Counterpart"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 1024
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 1,
                    "queuePrefix": "q1"
                }
            ]
        }
    ],
    "dimensions" : {
        "multipleDimensions": {
            "dimensions": ["PublisherRate","ProcessingMs"],
            "multiValues": [
                [100000,1000],
                [1,0]
            ],
            "stepDurationSeconds": 300,
            "rampUpSeconds": 5
        }
    }
}