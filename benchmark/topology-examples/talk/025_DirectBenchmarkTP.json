{
  "topologyName": "TP_DirectBenchmark2",
  "topologyType": "fixed",
  "benchmarkType": "throughput",
  "description": "5 routing keys, five queues",
  "vhosts": [
    {
      "name": "benchmark",
      "scale": 1,
      "exchanges": [
        {
          "name": "ex1",
          "type": "direct"
        }
      ],
      "queueGroups": [
        { "group": "q1", "scale": 1, "bindings": [ { "from": "ex1", "bindingKey": "rkey1" } ] }
      ],
      "publisherGroups": [
        {
          "group": "p1",
          "scale": 1,
          "sendToExchange": {
            "exchange": "ex1",
            "routingKeys": ["rkey1"],
            "routingKeyMode": "multivalue"
          },
          "deliveryMode": "Persistent"
        }
      ],
      "consumerGroups": [
        { "group": "c1", "scale": 1, "queueGroup": "q1" }
      ]
    }
  ],
  "dimensions" : {
    "fixedDimensions": {
      "durationSeconds": 300,
      "rampUpSeconds": 10
    }
  }
}