{
  "topologyName": "FastConsistentHash",
  "topologyType": "fixed",
  "benchmarkType": "throughput",
  "topologyGroups": [
    {
      "name": "benchmark",
      "scale": 1,
      "exchanges": [
        {
          "name": "sharded",
          "type": "modulushash"
        }
      ],
      "queues": [
        {
          "prefix": "sharded",
          "scale": 1,
          "bindings": [
            {
              "from": "sharded"
            }
          ]
        }
      ],
      "publishers": [
        {
          "prefix": "p1",
          "scale": 10,
          "publishMode": {
            "useConfirms": true,
            "inFlightLimit": 10000
          },
          "sendToExchange": {
            "exchange": "sharded",
            "routingKeyMode": "random"
          },
          "deliveryMode": "Persistent"
        }
      ],
      "consumers": [
        {
          "prefix": "c1",
          "scale": 18,
          "queuePrefix": "sharded",
          "ackMode": {
            "manualAcks": true,
            "consumerPrefetch": 1000,
            "ackInterval": 100
        }
        }
      ]
    }
  ],
  "dimensions" : {
    "fixedDimensions": {
      "durationSeconds": 120,
      "rampUpSeconds": 10
    }
  }
}