{
    "topologyName": "TALK_020_MirroredRepFactor3MultiplePubQueueCon",
    "topologyType": "MultipleDimensions",
    "benchmarkType": "throughput",
    "description": "Quorum (3x), 1 kb, 500 msg/s, increasing pub/queue/consumer",
    "vhosts": [
        {
            "name": "benchmark",
            "scale": 1,
            "queueGroups": [
                {
                    "group": "q1",
                    "scale": 1,
                    "properties": [
                        { "key": "x-queue-type", "value": "quorum", "type": "string" },
                        { "key": "x-quorum-initial-group-size", "value": "5", "type": "int" }
                      ]
                }
            ],
            "publisherGroups": [
                {
                    "group": "p1",
                    "scale": 1,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 10000
                    },
                    "sendToQueueGroup": {
                        "queueGroup": "q1",
                        "mode": "Counterpart"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 1024
                }
            ],
            "consumerGroups": [
                {
                    "group": "c1",
                    "scale": 1,
                    "queueGroup": "q1",
                    "ackMode": {
                        "manualAcks": true,
                        "consumerPrefetch": 10000,
                        "ackInterval": 1
                    }
                }
            ]
        }
    ],
    "dimensions" : {
        "multipleDimensions": {
            "dimensions": ["Queues","Consumers", "Publishers"],
            "multiValues": [
                [1,1,1],
                [10,10,10],
                [20,20,20],
                [30,30,30],
                [40,40,40],
                [50,50,50],
                [60,60,60],
                [70,70,70],
                [80,80,80],
                [90,90,90],
                [100,100,100]
            ],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 5
        }
    }
}