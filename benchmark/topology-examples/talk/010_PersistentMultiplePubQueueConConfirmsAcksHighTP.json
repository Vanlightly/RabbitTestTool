{
    "topologyName": "TALK_009_PersistentMultiplePubQueueCon",
    "topologyType": "MultipleDimensions",
    "benchmarkType": "throughput",
    "description": "Persistent, 1 kb, 50 msg/s, increasing pub/queue/consumer",
    "vhosts": [
        {
            "name": "benchmark",
            "scale": 1,
            "queueGroups": [
                {
                    "group": "q1",
                    "scale": 1
                }
            ],
            "publisherGroups": [
                {
                    "group": "p1",
                    "scale": 1,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 10000
                    },
                    "sendToQueueGroup": {
                        "queueGroup": "q1",
                        "mode": "Counterpart"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 1024,
                    "msgsPerSecondPerPublisher": 1000
                }
            ],
            "consumerGroups": [
                {
                    "group": "c1",
                    "scale": 1,
                    "queueGroup": "q1",
                    "ackMode": {
                        "manualAcks": true,
                        "consumerPrefetch": 1000,
                        "ackInterval": 100
                    }
                }
            ]
        }
    ],
    "dimensions" : {
        "multipleDimensions": {
            "dimensions": ["Queues","Consumers", "Publishers"],
            "multiValues": [
                [1,1,1],
                [20,20,20],
                [40,40,40],
                [60,60,60],
                [80,80,80],
                [100,100,100],
                [120,120,120],
                [140,140,140],
                [160,160,160],
                [180,180,180],
                [200,200,200],
                [220,220,220],
                [240,240,240],
                [260,260,260],
                [280,280,280],
                [300,300,300],
                [320,320,320],
                [340,340,340],
                [360,360,360],
                [380,380,380],
                [400,400,400],
                [420,420,420],
                [440,440,440],
                [460,460,460],
                [480,480,480],
                [500,500,500]
            ],
            "stepDurationSeconds": 20,
            "rampUpSeconds": 5
        }
    }
}