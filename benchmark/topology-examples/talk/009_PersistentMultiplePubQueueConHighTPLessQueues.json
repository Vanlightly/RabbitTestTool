{
    "topologyName": "TALK_009_PersistentMultiplePubQueueCon",
    "topologyType": "MultipleDimensions",
    "benchmarkType": "throughput",
    "description": "Persistent, 1 kb, 50 msg/s, increasing pub/queue/consumer",
    "vhosts": [
        {
            "name": "benchmark",
            "scale": 1,
            "queueGroups": [
                {
                    "group": "q1",
                    "scale": 1
                }
            ],
            "publisherGroups": [
                {
                    "group": "p1",
                    "scale": 1,
                    "sendToQueueGroup": {
                        "queueGroup": "q1",
                        "mode": "Counterpart"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 1024,
                    "msgsPerSecondPerPublisher": 10000
                }
            ],
            "consumerGroups": [
                {
                    "group": "c1",
                    "scale": 1,
                    "queueGroup": "q1"
                }
            ]
        }
    ],
    "dimensions" : {
        "multipleDimensions": {
            "dimensions": ["Queues","Consumers", "Publishers"],
            "multiValues": [
                [1,1,1],
                [10,10,10],
                [20,20,20],
                [30,30,30],
                [40,40,40],
                [50,50,50],
                [60,60,60],
                [70,70,70],
                [80,80,80],
                [90,90,90],
                [100,100,100]
            ],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 5
        }
    }
}