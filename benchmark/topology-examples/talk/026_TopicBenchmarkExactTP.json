{
  "topologyName": "TP_DirectBenchmark2",
  "topologyType": "fixed",
  "benchmarkType": "throughput",
  "description": "5 routing keys, five queues",
  "topologyGroups": [
    {
      "name": "benchmark",
      "scale": 1,
      "exchanges": [
        {
          "name": "ex1",
          "type": "topic"
        }
      ],
      "queues": [
        { "prefix": "q1", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["rkey1"] } ] }
      ],
      "publishers": [
        {
          "prefix": "p1",
          "scale": 1,
          "sendToExchange": {
            "exchange": "ex1",
            "routingKeys": ["rkey1"],
            "routingKeyMode": "multivalue"
          },
          "deliveryMode": "Persistent"
        }
      ],
      "consumers": [
        { "prefix": "c1", "scale": 1, "queuePrefix": "q1" }
      ]
    }
  ],
  "dimensions" : {
    "fixedDimensions": {
      "durationSeconds": 300,
      "rampUpSeconds": 10
    }
  }
}