{
    "topologyName": "QueueRecovery3",
    "topologyType": "MultipleDimensions",
    "benchmarkType": "throughput",
    "description": "Prepublish 300000, consume with slow start",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "queues": [{ "prefix": "q1", "scale": 1 }],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 1000
                    },
                    "sendToQueuePrefix": {
                        "queuePrefix": "q1",
                        "mode": "Counterpart"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 1024,
                    "initialPublish": 300000,
                    "msgsPerSecondPerPublisher": 2
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 1,
                    "queuePrefix": "q1",
                    "ProcessingMs": 1000,
                    "ackMode": {
                        "manualAcks": true,
                        "consumerPrefetch": 1,
                        "ackInterval": 1
                    }
                }
            ]
        }
    ],
    "dimensions" : {
        "multipleDimensions": {
            "dimensions": ["Prefetch","ProcessingMs"],
            "multiValues": [
                [1, 1000],
                [100, 0]
            ],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 5
        }
    }
}