{
  "topologyName": "LT_MirroredQueueBenchmark1",
  "topologyType": "fixed",
  "benchmarkType": "latency",
  "description": "Persistent messages, replication of 3",
  "topologyGroups": [
    {
      "name": "benchmark",
      "scale": 1,
      "exchanges": [
        {
          "name": "ex1",
          "type": "fanout"
        }
      ],
      "queues": [
        {
          "prefix": "q1",
          "scale": 1,
          "bindings": [
            {
              "from": "ex1"
            }
          ],
          "properties": [
            { "key": "ha-mode", "value": "exactly", "type": "string" },
            { "key": "ha-params", "value": "3", "type": "int" },
            { "key": "ha-sync-mode", "value": "automatic", "type": "string" }
          ]
        }
      ],
      "publishers": [
        {
          "prefix": "p1",
          "scale": 1,
          "sendToExchange": {
            "exchange": "ex1",
            "routingKeyMode": "none"
          },
          "deliveryMode": "Persistent",
          "msgsPerSecondPerPublisher": 100
        }
      ],
      "consumers": [
        {
          "prefix": "c1",
          "scale": 1,
          "queuePrefix": "q1"
        }
      ]
    }
  ],
  "dimensions" : {
    "fixedDimensions": {
      "durationSeconds": 120,
      "rampUpSeconds": 10
    }
  }
}