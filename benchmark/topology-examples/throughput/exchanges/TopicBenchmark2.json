{
    "topologyName": "TP_TopicBenchmark2",
    "topologyType": "SingleDimension",
    "benchmarkType": "throughput",
    "description": "* match, growing routing key length",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "exchanges": [
                { "name": "ex1", "type": "topic" }
            ],
            "queues": [
                { "prefix": "q1", "scale": 1, "bindings": [{ "from": "ex1", "bindingKeys": ["a.*.c"] }] },
                { "prefix": "q2", "scale": 1, "bindings": [{ "from": "ex1", "bindingKeys": ["aaaaaaaaaa.*.cccccccccc"] }] },
                { "prefix": "q3", "scale": 1, "bindings": [{ "from": "ex1", "bindingKeys": ["aaaaaaaaaaaaaaaaaaaa.*.cccccccccccccccccccc"] }] },
                { "prefix": "q4", "scale": 1, "bindings": [{ "from": "ex1", "bindingKeys": ["aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa.*.cccccccccccccccccccccccccccccccccccccccc"] }] },
                { "prefix": "q5", "scale": 1, "bindings": [{ "from": "ex1", "bindingKeys": ["aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa.*.cccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccc"] }] }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "sendToExchange": {
                        "exchange": "ex1",
                        "routingKeys": [
                            "a.b.c",
                            "aaaaaaaaaa.bbbbbbbbbb.cccccccccc",
                            "aaaaaaaaaaaaaaaaaaaa.bbbbbbbbbbbbbbbbbbbb.cccccccccccccccccccc",
                            "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa.bbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbb.cccccccccccccccccccccccccccccccccccccccc",
                            "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa.bbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbb.cccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccc"],
                        "routingKeyMode": "index"
                    },
                    "deliveryMode": "Persistent"
                }
            ],
            "consumers": [
                { "prefix": "c1", "scale": 1, "queuePrefix": "q1" },
                { "prefix": "c2", "scale": 1, "queuePrefix": "q2" },
                { "prefix": "c3", "scale": 1, "queuePrefix": "q3" },
                { "prefix": "c4", "scale": 1, "queuePrefix": "q4" },
                { "prefix": "c5", "scale": 1, "queuePrefix": "q5" }
            ]
        }
    ],
    "dimensions" : {
        "singleDimension": {
            "dimension": "RoutingKeyIndex",
            "values": [0, 1, 2, 3, 4],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10,
            "applyToGroup": "p1"
        }
    }
}