{
  "topologyName": "TP_VhostBenchmark4",
  "topologyType": "fixed",
  "benchmarkType": "throughput",
  "description": "1 vhosts, 50 queues with 1kb persistent messages",
  "topologyGroups": [
    {
      "name": "benchmark",
      "scale": 1,
      "queues": [
        {
          "prefix": "q1",
          "scale": 50
        }
      ],
      "publishers": [
        {
          "prefix": "p1",
          "scale": 50,
          "sendToQueuePrefix": {
            "queuePrefix": "q1",
            "mode": "Counterpart"
          },
          "deliveryMode": "Persistent",
          "messageSize": 1024
        }
      ],
      "consumers": [
        {
          "prefix": "c1",
          "scale": 50,
          "queuePrefix": "q1"
        }
      ]
    }
  ],
  "dimensions" : {
    "fixedDimensions": {
      "durationSeconds": 120,
      "rampUpSeconds": 10
    }
  }
}