{
  "topologyName": "TP_LongRunningBenchmark5",
  "topologyType": "fixed",
  "benchmarkType": "throughput",
  "description": "64 Kb persistent messages over 1 hour, 30 pub/queue/con",
  "topologyGroups": [
    {
      "name": "benchmark",
      "scale": 1,
      "queues": [
        {
            "prefix": "q1",
            "scale": 30
        }
      ],
      "publishers": [
          {
              "prefix": "p1",
              "scale": 30,
              "sendToQueuePrefix": {
                  "queuePrefix": "q1",
                  "mode": "random"
              },
              "deliveryMode": "Persistent",
              "messageSize": 65536
          }
      ],
      "consumers": [
          {
              "prefix": "c1",
              "scale": 30,
              "queuePrefix": "q1"
          }
      ]
    }
  ],
  "dimensions" : {
    "fixedDimensions": {
      "durationSeconds": 3600,
      "rampUpSeconds": 10
    }
  }
}