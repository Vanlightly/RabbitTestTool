{
    "topologyName": "TP_SafetyBenchmark5",
    "topologyType": "MultipleDimensions",
    "benchmarkType": "throughput",
    "description": "1kb msgs. Confirms (1000) and manual acks (500/1). Scale out fanout",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "exchanges": [
                {
                    "name": "ex1",
                    "type": "fanout"
                }
            ],
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 1,
                    "bindings": [
                        { "from": "ex1" }
                    ]
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 1000
                    },
                    "sendToExchange": {
                        "exchange": "ex1",
                        "routingKeyMode": "none"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 1024
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 1,
                    "queuePrefix": "q1",
                    "ackMode": {
                        "manualAcks": true,
                        "consumerPrefetch": 500,
                        "ackInterval": 1
                    }
                }
            ]
        }
    ],
    "dimensions" : {
        "multipleDimensions": {
            "dimensions": ["Queues", "Consumers"],
            "multiValues": [
                [1,1],
                [2,2],
                [4,4],
                [8,8],
                [16,16],
                [32,32]
            ],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10
        }
    }
}