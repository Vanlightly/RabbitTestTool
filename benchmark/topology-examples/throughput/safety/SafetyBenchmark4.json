{
    "topologyName": "TP_SafetyBenchmark4",
    "topologyType": "MultipleDimensions",
    "benchmarkType": "throughput",
    "description": "1kb msgs. Confirms (1000) and manual acks (500/1). Scale out pub/queue/con",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 1
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 1000
                    },
                    "sendToQueuePrefix": {
                        "queuePrefix": "q1",
                        "mode": "random"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 1024
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 1,
                    "queuePrefix": "q1",
                    "ackMode": {
                        "manualAcks": true,
                        "consumerPrefetch": 500,
                        "ackInterval": 1
                    }
                }
            ]
        }
    ],
    "dimensions" : {
        "multipleDimensions": {
            "dimensions": ["Queues","Consumers", "Publishers"],
            "multiValues": [
                [1,1,1],
                [2,2,2],
                [4,4,4],
                [8,8,8],
                [16,16,16],
                [32,32,32]
            ],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10
        }
    }
}