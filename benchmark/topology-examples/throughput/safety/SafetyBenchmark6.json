{
    "topologyName": "TP_SafetyBenchmark6",
    "topologyType": "SingleDimension",
    "benchmarkType": "throughput",
    "description": "1kb msgs. Sacle out confirms in-flight limit and manual acks (500/1). 20 pub, 1 queue/con",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 1
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 20,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 1
                    },
                    "sendToQueuePrefix": {
                        "queuePrefix": "q1",
                        "mode": "random"
                    },
                    "deliveryMode": "Persistent",
                    "messageSize": 1024
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 1,
                    "queuePrefix": "q1",
                    "ackMode": {
                        "manualAcks": true,
                        "consumerPrefetch": 500,
                        "ackInterval": 1
                    }
                }
            ]
        }
    ],
    "dimensions" : {
        "singleDimension": {
            "dimension": "InFlightLimit",
            "values": [1, 10, 20, 30, 40, 50, 100, 150, 200, 250, 300, 350, 400, 450, 500, 1000, 2000, 4000],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10
        }
    }
}