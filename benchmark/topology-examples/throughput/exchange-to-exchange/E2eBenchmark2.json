{
    "topologyName": "TP_E2eBenchmark2",
    "topologyType": "Fixed",
    "benchmarkType": "throughput",
    "description": "ex->ex->queue",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "exchanges": [
                { "name": "ex1","type": "fanout" },
                { "name": "ex2","type": "fanout", "bindings": [ { "from": "ex1" }] }
            ],
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 1,
                    "bindings": [
                        { "from": "ex2" }
                    ]
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "sendToExchange": {
                        "exchange": "ex1",
                        "routingKeyMode": "none"
                    },
                    "deliveryMode": "Persistent"
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 1,
                    "queuePrefix": "q1"
                }
            ]
        }
    ],
    "dimensions" : {
        "fixedDimensions": {
            "durationSeconds": 120,
            "rampUpSeconds": 10
        }
    }
}