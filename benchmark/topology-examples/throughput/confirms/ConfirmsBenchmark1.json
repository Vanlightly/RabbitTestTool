{
    "topologyName": "TP_ConfirmsBenchmark1",
    "topologyType": "SingleDimension",
    "benchmarkType": "throughput",
    "description": "Increasing in-flight-limit, 1 pub/queue,con",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 1
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 1
                    },
                    "sendToQueuePrefix": {
                        "queuePrefix": "q1",
                        "mode": "CounterPart"
                    },
                    "deliveryMode": "Persistent"
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 1,
                    "queuePrefix": "q1"
                }
            ]
        }
    ],
    "dimensions" : {
        "singleDimension": {
            "dimension": "InFlightLimit",
            "values": [1, 5, 10, 50, 100, 500],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10,
            "applyToGroup": "p1"
        }
    }
}