{
    "topologyName": "TP_ConfirmsBenchmark3",
    "topologyType": "SingleDimension",
    "benchmarkType": "throughput",
    "description": "Increasing in-flight-limit, 1 pub, direct, round-robin to 10 queues",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "exchanges": [
                {
                    "name": "ex1",
                    "type": "direct"
                }
            ],
            "queues": [
                { "prefix": "q1", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["q1"] } ] },
                { "prefix": "q2", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["q2"] } ] },
                { "prefix": "q3", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["q3"] } ] },
                { "prefix": "q4", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["q4"] } ] },
                { "prefix": "q5", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["q5"] } ] },
                { "prefix": "q6", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["q6"] } ] },
                { "prefix": "q7", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["q7"] } ] },
                { "prefix": "q8", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["q8"] } ] },
                { "prefix": "q9", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["q9"] } ] },
                { "prefix": "q10", "scale": 1, "bindings": [ { "from": "ex1", "bindingKeys": ["q10"] } ] }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "publishMode": {
                        "useConfirms": true,
                        "inFlightLimit": 1
                    },
                    "sendToExchange": {
                        "exchange": "ex1",
                        "routingkeys": ["q1", "q2", "q3", "q4", "q5", "q6", "q7", "q8", "q9", "q10"],
                        "routingKeyMode": "multivalue"
                    },
                    "deliveryMode": "Persistent"
                }
            ],
            "consumers": [ 
                { "prefix": "c1", "scale": 1, "queuePrefix": "q1" },
                { "prefix": "c2", "scale": 1, "queuePrefix": "q2" },
                { "prefix": "c3", "scale": 1, "queuePrefix": "q3" },
                { "prefix": "c4", "scale": 1, "queuePrefix": "q4" },
                { "prefix": "c5", "scale": 1, "queuePrefix": "q5" },
                { "prefix": "c6", "scale": 1, "queuePrefix": "q6" },
                { "prefix": "c7", "scale": 1, "queuePrefix": "q7" },
                { "prefix": "c8", "scale": 1, "queuePrefix": "q8" },
                { "prefix": "c9", "scale": 1, "queuePrefix": "q9" },
                { "prefix": "c10", "scale": 1, "queuePrefix": "q10" }
            ]
        }
    ],
    "dimensions" : {
        "singleDimension": {
            "dimension": "InFlightLimit",
            "values": [1, 5, 10, 50, 100, 500, 1000, 5000],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10,
            "applyToGroup": "p1"
        }
    }
}