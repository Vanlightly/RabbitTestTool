{
    "topologyName": "TP_PrefetchAckIntervalBenchmark2.json",
    "topologyType": "SingleDimension",
    "benchmarkType": "throughput",
    "description": "Increasing prefetch, 1 ack interval",
    "vhosts": [
        {
            "name": "benchmark",
            "scale": 1,
            "exchanges": [
                {
                    "name": "ex1",
                    "type": "fanout"
                }
            ],
            "queueGroups": [
                {
                    "group": "q1",
                    "scale": 1,
                    "bindings": [
                        { "from": "ex1" }
                    ]
                }
            ],
            "publisherGroups": [
                {
                    "group": "p1",
                    "scale": 1,
                    "sendToExchange": {
                        "exchange": "ex1",
                        "routingKeyMode": "none"
                    },
                    "deliveryMode": "Persistent"
                }
            ],
            "consumerGroups": [
                {
                    "group": "c1",
                    "scale": 1,
                    "queueGroup": "q1",
                    "ackMode": {
                        "manualAcks": true,
                        "consumerPrefetch": 1,
                        "ackInterval": 1
                    }
                }
            ]
        }
    ],
    "dimensions" : {
        "singleDimension": {
            "dimension": "Prefetch",
            "values": [1, 5, 10, 50, 100, 500],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10,
            "applyToGroup": "c1"
        }
    }
}