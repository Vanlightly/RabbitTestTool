{
    "topologyName": "TP_ClientsBenchmark5",
    "topologyType": "MultipleDimensions",
    "benchmarkType": "throughput",
    "description": "Default ex, increasing publishers, queues, consumers",
    "topologyGroups": [
        {
            "name": "benchmark",
            "scale": 1,
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 1
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "sendToQueuePrefix": {
                        "queuePrefix": "q1",
                        "mode": "random"
                    },
                    "deliveryMode": "Persistent"
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 1,
                    "queuePrefix": "q1"
                }
            ]
        }
    ],
    "dimensions" : {
        "multipleDimensions": {
            "dimensions": ["Queues","Consumers", "Publishers"],
            "multiValues": [
                [1,1,1],
                [2,2,2],
                [4,4,4],
                [8,8,8],
                [16,16,16],
                [32,32,32]
            ],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10
        }
    }
}