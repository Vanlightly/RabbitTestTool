{
    "topologyName": "TP_ClientsBenchmark3",
    "topologyType": "MultipleDimensions",
    "benchmarkType": "throughput",
    "description": "Increasing publishers and consumers on 1 queue",
    "topologyGroups": [
        {
          "name": "benchmark",
          "scale": 1,
            "exchanges": [
                {
                    "name": "ex1",
                    "type": "fanout"
                }
            ],
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 1,
                    "bindings": [
                        { "from": "ex1" }
                    ]
                }
            ],
            "publishers": [
                {
                    "prefix": "p1",
                    "scale": 1,
                    "sendToExchange": {
                        "exchange": "ex1",
                        "routingKeyMode": "none"
                    },
                    "deliveryMode": "Persistent"
                }
            ],
            "consumers": [
                {
                    "prefix": "c1",
                    "scale": 1,
                    "queuePrefix": "q1"
                }
            ]
        }
    ],
    "dimensions" : {
        "multipleDimensions": {
            "dimensions": ["Consumers", "Publishers"],
            "multiValues": [
                [1,1],
                [2,2],
                [4,4],
                [8,8],
                [16,16],
                [32,32]
            ],
            "stepDurationSeconds": 60,
            "rampUpSeconds": 10,
            "applyToGroup": "c1"
        }
    }
}