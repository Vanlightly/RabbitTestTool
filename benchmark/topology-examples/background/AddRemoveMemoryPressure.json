    "topologyGroups": [
        {
            "name": "background",
            "scale": 1,
            "queues": [
                {
                    "prefix": "q1",
                    "scale": 10,
                    "properties": [
                        { "key": "x-queue-type", "value": "quorum", "type": "string" },
                        { "key": "x-quorum-initial-group-size", "value": "3", "type": "int" }
